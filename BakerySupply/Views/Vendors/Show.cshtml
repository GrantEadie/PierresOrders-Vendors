@using BakerySupply.Models;
@{
  Layout = "_Layout";
}

<h3>here are all the orders for this vendor:</h3>

<ol>
@foreach (Order order in @Model["orders"])
{
  <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.Item</a></li>
}
</ol>

<p><a href='/vendors/@Model["vendor"].Id/orders/new'>add a new order by this vendor</a></p>
<p><a href='/vendors'>return to vendor list</a></p>
<p><a href='/'>return to main page</a></p>
<form action="/items/delete" method="post">
  <button type="submit" name="button">Clear All Items</button>
</form>